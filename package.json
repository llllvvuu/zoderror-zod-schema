{
  "name": "@llllvvuu/zoderror-zod-schema",
  "author": "@llllvvuu",
  "license": "ISC",
  "version": "0.1.6",
  "description": "Zod schema matching the z.ZodError class.",
  "keywords": [
    "zod",
    "codegen",
    "openapi"
  ],
  "homepage": "https://github.com/llllvvuu/zoderror-zod-schema",
  "bugs": "https://github.com/llllvvuu/zoderror-zod-schema/issues",
  "repository": "github:llllvvuu/zoderror-zod-schema",
  "files": [
    "dist",
    "lib.ts"
  ],
  "main": "dist/cjs/lib.cjs",
  "types": "dist/cjs/lib.d.cts",
  "module": "dist/esm/lib.mjs",
  "exports": {
    ".": {
      "import": "./dist/esm/lib.mjs",
      "require": "./dist/cjs/lib.cjs"
    }
  },
  "sideEffects": false,
  "packageManager": "pnpm@8.6.12",
  "scripts": {
    "build": "sh build.sh",
    "prettier": "prettier --write .",
    "prePublishOnly": "pnpm build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "rimraf": "^5.0.1",
    "zod": "^3.22.2"
  },
  "peerDependencies": {
    "zod": "^3.22.2"
  }
}
